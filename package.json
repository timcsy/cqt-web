    "spectrogram",
    "frequency-analysis",
    "vqt",
    "fft",
    "audio-analysis",
    "music-information-retrieval"
  ],
  "author": "timcsy",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timcsy/cqt-web.git"
  },
  "bugs": {
    "url": "https://github.com/timcsy/cqt-web/issues"
  },
  "homepage": "https://github.com/timcsy/cqt-web#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "browser": {
    "fs": false,
    "path": false
